@import "reset.css";
.outPage {
  width: 120rem;
  height: 150rem;
  background-color: rgb(63, 63, 124);
  color: rgb(250, 250, 250);
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
}
.outPage .errorArticle {
  width: 60rem;
  height: 20rem;
  position: absolute;
  background: rgba(250, 250, 250, 0.85);
  border-radius: 5rem;
  box-shadow: 0rem 0rem 10rem rgba(73, 73, 73, 0.85);
  color: rgb(49, 49, 49);
  font-size: 4rem;
  display: flex;
  text-align: center;
  justify-content: center;
  align-items: center;
  font-weight: 700;
  visibility: hidden;
  transition: all 0.5s linear;
  color: rgb(231, 67, 89);
}
.outPage .hiddenDiv {
  position: absolute;
}
.outPage .topSection {
  height: 75rem;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
}
.outPage .topSection .headerArticle {
  display: flex;
  justify-content: flex-start;
  height: 10rem;
  width: 100%;
}
.outPage .topSection .headerArticle article:first-child {
  width: 5rem;
}
.outPage .topSection .headerArticle article:nth-child(2) {
  width: 10rem;
  font-size: 3.8rem;
  text-align: center;
  cursor: default;
}
.outPage .topSection .headerArticle article:nth-child(3) {
  width: 31rem;
}
.outPage .topSection .headerArticle article:nth-child(5) {
  width: 31rem;
}
.outPage .topSection .headerArticle article:nth-child(7) {
  width: 5rem;
}
.outPage .topSection .headerArticle header {
  width: 28rem;
  height: 10rem;
  display: flex;
  justify-content: center;
  align-items: flex-end;
  line-height: 13rem;
  font-size: 7rem;
  font-weight: 700;
}
.outPage .topSection .headerArticle .exitIcon {
  display: flex;
  font-size: 10rem;
}
.outPage .topSection .parkingStatus {
  width: 100rem;
  height: 60rem;
  background: rgb(235, 235, 235);
  border-radius: 1rem;
}
.outPage .topSection .parkingStatus .parkingArticleOne {
  justify-content: flex-start;
}
.outPage .topSection .parkingStatus .parkingArticleFour {
  justify-content: flex-start;
}
.outPage .topSection .parkingStatus article {
  width: 100%;
  height: 13rem;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.outPage .topSection .parkingStatus article .loadBox {
  background: rgb(99, 99, 99);
  width: 4rem;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.outPage .topSection .parkingStatus article .loadBox .arrowIcon {
  color: rgb(250, 250, 250);
  font-size: 4rem;
  font-weight: 700;
  margin-bottom: 0.5rem;
}
.outPage .topSection .parkingStatus article .loadBox .topArrow {
  margin-top: 0.5rem;
}
.outPage .topSection .parkingStatus article .topLoadBox {
  border-radius: 0.8rem 0rem 0 0;
  align-items: flex-start;
}
.outPage .topSection .parkingStatus article .buttomLoadBox {
  border-radius: 0 0 0rem 0.8rem;
}
.outPage .topSection .parkingStatus article article {
  width: 92rem;
  height: 100%;
  justify-content: space-around;
  align-items: center;
}
.outPage .topSection .parkingStatus article article .parkingBox {
  width: 10rem;
  height: 9rem;
  background: rgb(107, 211, 112);
  box-shadow: inset -0.3rem -0.3rem 1rem 0.2rem rgba(233, 233, 233, 0.2941176471);
  border-radius: 2rem;
  border: 1rem solid rgb(250, 250, 250);
  font-size: 5rem;
  display: flex;
  justify-content: center;
  line-height: 8.5rem;
  color: rgb(250, 250, 250);
}
.outPage .topSection .parkingStatus .loadArticle {
  height: 4rem;
  width: 100%;
  background: rgb(99, 99, 99);
}
.outPage .topSection .parkingStatus .topLoadArticle {
  border-radius: 0rem 0.8rem 0 0.8rem;
}
.outPage .topSection .parkingStatus .bottomLoadArticle {
  border-radius: 0.8rem 0rem 0.8rem 0rem;
}
.outPage .midSection {
  height: 25rem;
  width: 100%;
  display: flex;
  flex-flow: column;
}
.outPage .midSection .timeArticle {
  width: 100%;
  height: 50%;
  display: flex;
  flex-flow: column;
}
.outPage .midSection .timeArticle .timeAndPriceArticle {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  width: 100%;
}
.outPage .midSection .timeArticle .timeAndPriceArticle .timeTextBox {
  font-size: 3.5rem;
  text-align: center;
  height: 10rem;
  display: flex;
  align-items: center;
  justify-content: center;
}
.outPage .midSection .timeArticle .timeAndPriceArticle .timeBox {
  font-size: 4rem;
  text-align: center;
  height: 8rem;
  display: flex;
  justify-content: center;
  border: none;
  width: 20rem;
  box-shadow: inset 0.8rem 0.8rem 2rem 0.4rem rgba(71, 71, 71, 0.35);
  background-color: rgb(250, 250, 250);
  color: rgb(49, 49, 49);
  border-radius: 2rem;
  cursor: default;
}
.outPage .midSection .timeArticle .inputArticle {
  width: 100%;
  height: 10rem;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  font-size: 5rem;
}
.outPage .midSection .timeArticle .inputArticle .messageBox {
  width: 68rem;
  height: 10rem;
  background: rgba(250, 250, 250, 0.9);
  position: absolute;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  margin-top: -10rem;
  margin-left: -25rem;
  color: rgb(49, 49, 49);
  border-radius: 2rem 2rem 0 0;
  font-size: 3rem;
  font-weight: 500;
  visibility: hidden;
}
.outPage .midSection .timeArticle .inputArticle input {
  width: 70rem;
  height: 10rem;
  outline: none;
  border: none;
  font-size: 3.5rem;
  padding-left: 2rem;
  border-radius: 2rem;
  box-shadow: inset 0.8rem 0.8rem 2rem 0.4rem rgba(71, 71, 71, 0.35);
  background-color: rgb(250, 250, 250);
  color: rgb(49, 49, 49);
}
.outPage .midSection .timeArticle .inputArticle .outButton {
  margin-left: 5rem;
  width: 20rem;
  height: 10rem;
  border: none;
  font-size: 4rem;
  border-radius: 2rem;
  padding-left: 0rem;
  text-align: center;
  visibility: hidden;
  box-shadow: inset 0.8rem 0.8rem 2rem 0.1rem rgba(255, 255, 255, 0.25);
  background-color: rgb(228, 100, 117);
  color: rgb(250, 250, 250);
}
.outPage .midSection .timeArticle .inputArticle .hiddenInput {
  width: 0rem;
  height: 0rem;
  color: transparent;
}
.outPage .bottomSection {
  height: 50rem;
  width: 100%;
  display: flex;
  justify-content: center;
  background: rgb(210, 210, 210);
}
.outPage .bottomSection .keyPad {
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
}
.outPage .bottomSection .keyPad .keyPadThree {
  margin-bottom: 1rem;
}
.outPage .bottomSection .keyPad article {
  width: 100%;
  height: 9rem;
  display: flex;
  justify-content: center;
  align-items: center;
}
.outPage .bottomSection .keyPad article :last-child {
  margin-right: 0rem;
}
.outPage .bottomSection .keyPad article button {
  width: 11rem;
  height: 8.5rem;
  margin-right: 0.5rem;
  border: none;
  font-size: 5rem;
  border-radius: 2rem;
  background: rgb(245, 245, 245);
  box-shadow: inset 0.5rem 0.5rem 2rem 0.3rem rgba(169, 169, 169, 0.25);
}
.outPage .bottomSection .keyPad article .enterButton {
  width: 70rem;
  height: 9rem;
  color: rgb(250, 250, 250);
  background-color: rgb(110, 124, 212);
  box-shadow: inset 0.8rem 0.8rem 2rem 0.2rem rgba(255, 255, 255, 0.25);
}
.outPage .bottomSection .keyPad article .resetButton {
  width: 18rem;
  height: 9rem;
  margin-right: 2rem;
}
.outPage .bottomSection .keyPad article .deleteButton {
  width: 18rem;
  height: 9rem;
  margin-left: 2rem;
  display: flex;
  justify-content: center;
  align-items: center;
}
.outPage .bottomSection .keyPad article .deleteButton div {
  background: url("../public/img/backspace.svg") no-repeat;
  width: 8rem;
  height: 8rem;
}